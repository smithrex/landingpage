<h1>Staff</h1>

<table class="table">
  <thead>
    <tr>
      <th>Id</th>
      <th>Email</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% @usuarios.each do |t| %>
    <tr>
        <td><%= t.id %></td>
        <td><%= t.email %></td>
        <td><a href="/admin/users/<%= t.id %>/show">Mostrar</a></td>
        <td><a href="/admin/users/<%= t.id %>/edit">Editar</a></td>
        <td><a href="/admin/users/<%= t.id %>/destroy">Eliminar</a></td>
    </tr>
    <% end %>
  </tbody>
</table>
